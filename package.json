{
  "name": "scroll-kit",
  "version": "0.3.5",
  "description": "Take care of your elements on the viewport",
  "main": "scroll-kit.js",
  "scripts": {
    "dev": "./node_modules/coffee-script/bin/coffee --watch --output . --compile *.coffee",
    "dist": "./node_modules/coffee-script/bin/coffee --output . --compile *.coffee",
    "lint": "./node_modules/coffeelint/bin/coffeelint *.coffee",
    "bower": "bower install --force --production --no-interactive",
    "nahual": "nahual -ST saucelabs -- python -m SimpleHTTPServer"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/gextech/scroll-kit.git"
  },
