    "Juan Carlos Teofilo <jcteo87@gmail.com>",
    "Juan Mendivil <juan@mendivil.mx>"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.7.1",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.12.1",
    "nahual": "0.0.12",
    "nightwatch": "^0.9.7"
  }
}
